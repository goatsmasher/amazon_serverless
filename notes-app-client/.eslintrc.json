{
	"env": {
		"browser": true,
		"node": true,
		"commonjs": true,
		"es6": true,
		"jquery": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"no-console": 0,
		"no-undef": 0,
		"linebreak-style": [
			"error",
			"windows"
		],
		"no-unused-vars": 0,
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		]
	}
}